generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x", "linux-arm64-openssl-1.1.x", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model account {
  id                String  @id
  userId            String
  type              String
  provider          String
  providerAccountId String
  refresh_token     String?
  access_token      String?
  expires_at        Int?
  token_type        String?
  scope             String?
  id_token          String?
  session_state     String?
  user              user    @relation(fields: [userId], references: [id], onDelete: Cascade, map: "Account_userId_fkey")

  @@unique([provider, providerAccountId], map: "Account_provider_providerAccountId_key")
  @@index([userId], map: "Account_userId_fkey")
}

model session {
  id           String   @id
  sessionToken String   @unique(map: "Session_sessionToken_key")
  userId       String
  expires      DateTime
  user         user     @relation(fields: [userId], references: [id], onDelete: Cascade, map: "Session_userId_fkey")

  @@index([userId], map: "Session_userId_fkey")
}

model user {
  id                String    @id @default(cuid())
  name              String?
  email             String?
  password          String?
  contactNo         String?   
  address           String?
  state             String?
  zipCode           String?
  country           String?
  language          String?
  companyName       String?
  roleId            String?      @default("3")
  roleName          String?      @default("customer")
  profileStatus     String?
  image             String?
  isDeleted         String?      @default("N")
  createdBy         String?
  subscriptionPlan  String?
  account           account[]
  session           session[]
  createdAt         DateTime           @default(now())
  updatedAt         DateTime           @updatedAt
}

model verification_token {
  identifier String
  token      String   @unique(map: "VerificationToken_token_key")
  expires    DateTime

  @@unique([identifier, token], map: "VerificationToken_identifier_token_key")
}

model role {
    id              String @id @default(cuid())
    userId          String
    roleName        String
    permissions     Json
    isDeleted       String?         @default("N")
    createdAt       DateTime           @default(now())
    updatedAt       DateTime           @updatedAt
}   

model menus {
    id              String @id @default(cuid())
    menuId          String?
    menu            String?
    subMenu         String?
    slugName        String?
    permission      Json?
    path            String?
    sequence        String?
    createdAt       DateTime           @default(now())
    updatedAt       DateTime           @updatedAt
}

model system_settings {
  id                String    @id @default(cuid())
  systemName        String?
  description       String?
  systemImage       String?
  navbarImage       String?
  metaTitle         String?
  email             String?
  contactNo         String?
  updatedUser       String?
  createdAt         DateTime           @default(now())
  updatedAt         DateTime           @updatedAt
}

model features {
  id                  String    @id @default(cuid())
  image               String?
  title               String?  
  description         String?  @db.VarChar(512)
  updatedUser         String?
  isDeleted           String?     @default("N")
  createdAt              DateTime  @default(now())
  updatedAt              DateTime  @updatedAt
}

model our_team {
  id                     String    @id @default(cuid())
  image                  String?
  title                  String?  
  description            String?  @db.VarChar(512)
  updatedUser            String?
  isDeleted           String?     @default("N")
  createdAt              DateTime  @default(now())
  updatedAt              DateTime  @updatedAt
}

model key_achievements {
  id          String   @id @default(cuid())
  image       String?
  keyMetric   String?
  highlight   String?
  isDeleted   String?  @default("N")
  createdUser String?
  updatedUser String?
  postedBy    String?  
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model faq {
  id                    String   @id @default(cuid())
  badgeTitle            String?  
  title                 String?  
  description           String?  @db.LongText
  createdAt             DateTime  @default(now())
  updatedAt             DateTime  @updatedAt
}

model questionanswer{
  id                    String   @id @default(cuid())
  question              String?
  answer                String?
  isDeleted             String?      @default("N")
  createdAt             DateTime  @default(now())
  updatedAt             DateTime  @updatedAt
}

model testimonial_reviews {
  id                     String    @id @default(cuid())
  image            String? 
  description      String?  @db.LongText
  rating           String?  
  postedBy         String?  
  designation      String?  
  isDeleted         String?      @default("N")
  createdAt        DateTime  @default(now())
  updatedAt        DateTime  @updatedAt
}

model contact_us {
  id                    String   @id @default(cuid())
  badgeTitle            String?  @db.LongText
  title                 String?  @db.LongText
  description           String?  @db.LongText
  cardTitle             String?  @db.LongText
  cardDescription       String?  @db.LongText
  isfrontendvisible     String?
  createdUser           String?
  updatedUser           String?
  createdAt             DateTime  @default(now())
  updatedAt             DateTime  @updatedAt
}

model customer_enquiries {
  id                    String    @id @default(cuid())
  name                  String?  
  email                 String?  
  message               String?   @db.LongText
  createdAt             DateTime  @default(now())
}

model brands {
  id                  String    @id @default(cuid())
  image               String? 
  createdAt           DateTime  @default(now())
  updatedAt           DateTime  @updatedAt
}

model subscribers {
  id                  String    @id @default(cuid())
  email               String? 
  createdAt           DateTime  @default(now())
  updatedAt           DateTime  @updatedAt
}

model country {
  id        Int      @id @default(autoincrement())
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model state {
  id        Int      @id @default(autoincrement())
  name      String 
  countryId Int
  country   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model city {
  id       Int     @id @default(autoincrement())
  name     String
  country  String
  countryId Int
  state    String
  stateId  Int
}

model district {
  id        Int      @id @default(autoincrement())
  name      String
  countryId Int    
  country   String
  stateId   Int  
  state     String
  cityId    Int   
  city      String
}

model section_content {
  id                    Int    @id @default(autoincrement())
  sectionType           String?
  badgeTitle            String?  
  title                 String?  
  description           String?  @db.VarChar(512)
  image1                String?
  image2                String?
  isfrontendvisible     String?   @default("Y")
  createdUser           String?
  updatedUser           String?
  createdAt             DateTime  @default(now())
  updatedAt             DateTime  @updatedAt
}

model package_plans {
  id                  String    @id @default(cuid())
  title               String? 
  subTitle            String?
  monthlyPrice        String? 
  yearlyPlan          Json?
  popularPlan         String?
  planBenefits        Json?
  image               String?
  createdUser         String?
  updatedUser         String?
  isDeleted        String?      @default("N")
  createdAt           DateTime  @default(now())
  updatedAt           DateTime  @updatedAt
}

model email_setting {
  id                  Int    @id @default(autoincrement())
  email               String? 
  emailType           String?
  password            String?
  createdUserId       String?
  updatedUserId       String?
  createdAt           DateTime  @default(now())
  updatedAt           DateTime  @updatedAt
}

model sent_emails {
  id                  Int       @id @default(autoincrement())
  from                String? 
  to                  String?   @db.LongText
  subject             String?   @db.LongText
  message             String?   @db.LongText
  sentBy              String?
  sendMailTo          String?
  createdAt           DateTime  @default(now())
}

model email_templates {
  id                  Int       @id @default(autoincrement())
  category            String? 
  slugCategory        String? 
  message             String?   @db.LongText
  isActive            String?
  createdBy           String?
  updatedBy           String?
  createdAt           DateTime  @default(now())
  updatedAt           DateTime  @updatedAt
}

model configure_subscription {
  id                 String    @id @default(cuid())
  name               String?  
  publicKey          String? 
  privateKey         String?
  isActive           String?   @default("N")
  type               String?
  createdUser        String?
  updatedUser        String?
  createdAt          DateTime  @default(now())
  updatedAt          DateTime  @updatedAt
}
